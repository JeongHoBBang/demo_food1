<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.green.demo_food.menu.MenuMapper">
    <insert id="insMenu">
        insert into d_menu
        set menu=#{menu}
        ,price=#{price}
        where icategory = icategory
    </insert>

</mapper>